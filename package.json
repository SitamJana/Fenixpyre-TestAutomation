{
  "name": "fenixpyre-testautomation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "report": "npx playwright show-report",
    "test:signin": "npm test -- --grep @signin",
    "test:signin:positive": "npm test -- --grep \"(?=.*@signin)(?=.*@positive)\"",
    "test:signin:negative": "npm test -- --grep \"(?=.*@signin)(?=.*@negative)\"",
    "test:fileActions": "npm test -- --grep @fileActions",
    "test:fileSearch": "npm test -- --grep @fileSearch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.45.3",
    "@types/node": "^20.14.12"
  },
  "dependencies": {
    "allure-playwright": "^3.0.0-beta.7",
    "easy-yopmail": "^5.1.0",
    "winston": "^3.13.1"
  }
}
